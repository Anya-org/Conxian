{
  "name": "conxian-stacks-tests",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
  "@hirosystems/clarinet-sdk": "3.5.0",
    "@types/node": "24.3.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.4.0",
    "vitest": "^3.2.4"
  },
  "scripts": {
    "test": "vitest run",
    "test:manual": "../scripts/manual-testing.sh",
